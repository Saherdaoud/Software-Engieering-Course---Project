<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.paint.Color?>
<?import javafx.scene.paint.LinearGradient?>
<?import javafx.scene.paint.Stop?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="558.0" prefWidth="838.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="il.cshaifasweng.OCSFMediatorExample.client.ShowListForSystemManagerController">
    <children>
        <ImageView fitHeight="558.0" fitWidth="838.0" layoutY="2.0" pickOnBounds="true" preserveRatio="true" AnchorPane.leftAnchor="0.0">
            <image>
                <Image url="@HomePage.jpg" />
            </image>
        </ImageView>
        <TableView fx:id="table" layoutX="44.0" layoutY="82.0" prefHeight="432.0" prefWidth="674.0" AnchorPane.leftAnchor="120.0" AnchorPane.rightAnchor="120.0">
            <columns>
                <TableColumn fx:id="username_column" prefWidth="145.0" text="username" />
                <TableColumn fx:id="name1_column" prefWidth="126.0" text="first name" />
                <TableColumn fx:id="name2_column" prefWidth="122.0" text="last name" />
                <TableColumn fx:id="type_column" minWidth="8.0" prefWidth="206.0" text="type/status" />
            </columns>
            <effect>
                <DropShadow>
                    <color>
                        <Color red="0.7803921699523926" green="0.05098039284348488" blue="0.49803921580314636" />
                    </color>
                </DropShadow>
            </effect>
        </TableView>
        <Button fx:id="back_button" layoutX="6.0" layoutY="7.0" mnemonicParsing="false" onAction="#back_butt" text="Back">
            <font>
                <Font name="Times New Roman Italic" size="15.0" />
            </font>
            <textFill>
                <LinearGradient endX="1.0" endY="1.0">
                    <stops>
                        <Stop>
                            <color>
                                <Color red="0.3176470696926117" green="0.0313725508749485" blue="0.3960784375667572" />
                            </color>
                        </Stop>
                        <Stop offset="1.0">
                            <color>
                                <Color red="0.7058823704719543" green="0.13333334028720856" blue="0.40392157435417175" />
                            </color>
                        </Stop>
                    </stops>
                </LinearGradient>
            </textFill></Button>
    </children>
</AnchorPane>
